raindrops <- function(num){
resp_text <- c("Pling", "Plang", "Plong")
resp_check <- c(num%%3 == 0, num%%5== 0, num%%7== 0)
if(sum(resp_check) == 0){
return(as.character(num))
}else{
return(paste(resp_text[resp_check], sep = "", collapse = ""))
}
}
##Checks
